<Window x:Class="HeBianGu.Product.WCFRemoteControl.RemoteControlApp.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HeBianGu.Product.WCFRemoteControl.RemoteControlApp"
        xmlns:screenclient="clr-namespace:HeBianGu.Product.WCFRemoteControl.Module.ScreenClient;assembly=HeBianGu.Product.WCFRemoteControl.Module.ScreenClient"
        xmlns:systemclient="clr-namespace:HeBianGu.Product.WCFRemoteControl.Module.SystemClient;assembly=HeBianGu.Product.WCFRemoteControl.Module.SystemClient"
        mc:Ignorable="d"
        Title="ShellWindow" Height="700" Width="1000" >
    <Grid>
        <TabControl>
            <TabItem Header="屏幕资源">
                <screenclient:ScreenUserControl/>
            </TabItem>

            <TabItem Header="系统命令">
                <systemclient:SystemUserControl/>
            </TabItem>
        </TabControl>
    
    </Grid>
</Window>
